<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>local.workstation.install.daily</string>
    <key>KeepAlive</key>
    <false/>
    <key>RunAtLoad</key>
    <false/>
    <key>UserName</key>
    <string>pivotal</string>
    <key>Program</key>
    <string>/Users/pivotal/workspace/networking-workspace/pull_install.sh</string>
    <key>StandardErrorPath</key>
    <string>/Users/pivotal/workspace/networking-workspace/install.daily.stderr.log</string>
    <key>StandardOutPath</key>
    <string>/Users/pivotal/workspace/networking-workspace/install.daily.stdout.log</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>12</integer>
        <key>Minute</key>
        <integer>30</integer>
    </dict>
</dict>
</plist>
